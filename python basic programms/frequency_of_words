

def freq_words() : 

    str = input("Enter a Sentence ")
    list =str.split()
    dict = {}
    for i in list :
        if i not in dict.keys() :
            dict[i]= 0
        dict[i]= dict[i] + 1
    print(dict)

    
    
# for i in li :
